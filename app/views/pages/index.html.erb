        </div>
        <div class="section-logos martop-small">
          <div class="logo-partners">
              <%= cl_image_tag("Nikita/Spotify_logo_black-250x75.png", :width=>125, :crop=>"scale") %>
          </div>
          <div class="logo-partners">
              <%= cl_image_tag("Nikita/applemusic0-250x59.png", :width=>125, :crop=>"scale") %>
          </div>
          <div class="logo-partners">
              <%= cl_image_tag("Nikita/GooglePlay.png", :width=>125, :crop=>"scale") %>
          </div>
          <div class="logo-partners">
              <%= cl_image_tag("Nikita/Amazon.png", :width=>125, :crop=>"scale")%>
          </div>
          <div class="logo-partners">
              <%= cl_image_tag("Nikita/YouTube.png", :width=>125, :crop=>"scale")%>
